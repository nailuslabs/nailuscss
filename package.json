{
  "name": "nailuscss",
  "version": "1.0.0",
  "description": "Instant on-demand atomic css generator engine (firts-utility).",
  "keywords": [
    "nailuscss",
    "nailuslabs",
    "tailwindcss",
    "unocss",
    "pandacss",
    "css",
    "javascript",
    "typescript",
    "python",
    "AI",
    "compiler"
  ],
  "license": "MIT",
  "author": "NaÃ¯loth Esdras Nsounda Nsilulu",
  "type": "module",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "bin": {
    "nailus": "./cli/index.cjs"
  },
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "rollup -cw",
    "watch": "concurrently -r \"npm run dev\" \"tsc -p tsconfig.dev.json --watch --noEmit\"",
    "build": "tsc -p tsconfig.build.json && rollup -c",
    "build:prod": "cross-env NODE_ENV=production pnpm build",
    "typecheck": "tsc -p tsconfig.dev.json --noEmit",
    "types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "lint": "eslint . --ext .ts",
    "lint:fix": "pnpm lint -- --fix",
    "test": "sucrase-node test/run.ts",
    "test:update": "cross-env UPDATE_SNAPSHOT=true pnpm test",
    "test:dts": "tsc -p test/dts --noEmit",
    "coverage": "nyc --nycrc-path=.nycrc.json pnpm test",
    "release": "bumpp --tag --commit --push"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./colors": {
      "import": "./colors.mjs",
      "require": "./colors.js"
    },
    "./defaultConfig": {
      "import": "./defaultConfig.mjs",
      "require": "./defaultConfig.js"
    },
    "./defaultTheme": {
      "import": "./defaultTheme.mjs",
      "require": "./defaultTheme.js"
    },
    "./config": {
      "import": "./config/index.mjs",
      "require": "./config/index.js"
    },
    "./lib": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./plugin": {
      "import": "./plugin/index.mjs",
      "require": "./plugin/index.js"
    },
    "./plugin/aspect-ratio": {
      "import": "./plugin/aspect-ratio/index.js",
      "require": "./plugin/aspect-ratio/index.js"
    },
    "./plugin/filters": {
      "import": "./plugin/filters/index.js",
      "require": "./plugin/filters/index.js"
    },
    "./plugin/forms": {
      "import": "./plugin/forms/index.js",
      "require": "./plugin/forms/index.js"
    },
    "./plugin/line-clamp": {
      "import": "./plugin/line-clamp/index.js",
      "require": "./plugin/line-clamp/index.js"
    },
    "./plugin/typography": {
      "import": "./plugin/typography/index.js",
      "require": "./plugin/typography/index.js"
    },
    "./plugin/scroll-snap": {
      "import": "./plugin/scroll-snap/index.js",
      "require": "./plugin/scroll-snap/index.js"
    },
    "./helpers": {
      "import": "./helpers/index.mjs",
      "require": "./helpers/index.js"
    },
    "./utils": {
      "import": "./utils/index.mjs",
      "require": "./utils/index.js"
    },
    "./utils/parser": {
      "import": "./utils/parser/index.mjs",
      "require": "./utils/parser/index.js"
    },
    "./utils/style": {
      "import": "./utils/style/index.mjs",
      "require": "./utils/style/index.js"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-sucrase": "^5.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/color-string": "^1.5.5",
    "@types/diff": "^7.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.12",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "arg": "^5.0.2",
    "bumpp": "^10.3.1",
    "caller-path": "^4.0.0",
    "color-string": "^2.1.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "diff": "^8.0.2",
    "domhandler": "^5.0.3",
    "eslint": "^9.38.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "jasmine": "^5.12.0",
    "jasmine-spec-reporter": "^7.0.0",
    "js-yaml": "^4.1.0",
    "mini-svg-data-uri": "^1.4.4",
    "minimatch": "^10.0.3",
    "nyc": "^17.1.0",
    "picocolors": "^1.1.1",
    "pirates": "^4.0.7",
    "rollup": "^4.52.5",
    "sucrase": "^3.35.0",
    "tosource": "^2.0.0-alpha.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
